  "buildCommand": "PARCEL_WORKER_BACKEND=process pnpm build",
  "devCommand": "pnpm parcel",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/site/index.ejs" },
    { "source": "/results", "destination": "/api/results" }
  ]
}
